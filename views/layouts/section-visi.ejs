        <ul class="title">
            <li>
                <h3>O</h3>
                <h1>VISI MISI </h1>
            </li>
        </ul>
        <ul class="visimisi-section">
        <div class="visi-section">
        <h5>Visi</h5>
        
        <% var i = 0 %> 
        <% isi1.forEach(element => { %>
            <ol>    
                    <p> <%= element["content"] %> </p>
                
            </ol>
        
        <% i++ %>
        <% }); %>
