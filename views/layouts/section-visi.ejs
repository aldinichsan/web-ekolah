        <% isi2.forEach(element => { %>
                    <ul>
                        <li>
                            <p class="misi"> <%= element["content"] %> </p>
                        </li>
                    </ul>
            
        <% i++ %>
        <% }); %> 
        </div>
        </ul>
        </main>

    <section>
        <div class="quote">
            <img src="quote2.jpg" alt="">
        </div>
        <div class="quote">
            <img src="quote1.jpg" alt="">
        </div>
    </section>
</div>